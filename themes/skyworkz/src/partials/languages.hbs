{{#section "languages"}}
<hr>
<section id="languages">
  {{#> header-languages }}
    {{> header-icon _title="languages" _section="languages" _hdricon=(lookup . '_icon') }}
  {{/header-languages}}
  <table>
    <thead>
      <tr>
        <th>Language</th>
        <th>Proficiency</th>
      </tr>
    </thead>
    <tbody>
      {{#each r.languages}}
      {{#> body-languages }}
      <tr>
        <td style="width: 20%;">{{{ language }}}</td>
        <td>{{{ fluency }}}</td>
      </tr>
      {{/body-languages}}
      {{/each}}
    </tbody>
  </table>
</section>
{{/section}}